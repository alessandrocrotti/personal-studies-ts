{
  "name": "personal-studies",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "dev:dsa-w3c": "nodemon --exec ts-node src/programming-principles/dsa/w3c/index.ts",
    "dev:design-patterns": "nodemon --exec ts-node src/programming-principles/design-patterns/<folder>/<file>.ts",
    "dev:solid": "nodemon --exec ts-node src/programming-principles/solid/<folder>/<file>.ts",
    "dev:rest-express": "nodemon --exec ts-node src/programming-principles/networking/api-paradigms/rest/express/index.ts",
    "dev:rest-fastify": "nodemon --exec ts-node src/programming-principles/networking/api-paradigms/rest/fastify/index.ts",
    "dev:grpc": "nodemon --exec ts-node src/programming-principles/networking/api-paradigms/grpc/index.ts",
    "generate:protos": "npx buf generate --template ./src/programming-principles/networking/api-paradigms/grpc/buf.gen.yaml",
    "dev:websocket": "nodemon --exec ts-node src/programming-principles/networking/api-paradigms/websocket/index.ts",
    "dev:webhook": "nodemon --exec ts-node src/programming-principles/networking/api-paradigms/webhook/index.ts",
    "dev:graphql": "nodemon --exec ts-node src/programming-principles/networking/api-paradigms/graphql/index.ts",
    "dev:apache-kafka": "nodemon --exec ts-node src/messaging-events/apache-kafka/lib/index.ts",
    "dev:rabbitmq": "nodemon --exec ts-node src/messaging-events/rabbitmq/lib/index.ts",
    "dev:redis": "nodemon --exec ts-node src/database/redis/lib/index.ts",
    "dev:mongodb-native": "nodemon --exec ts-node src/database/mongo/native-driver/index.ts",
    "prisma:generate": "prisma generate --schema=src/database/mysql/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/database/mysql/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=src/database/mysql/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/database/mysql/prisma/schema.prisma",
    "dev:prisma": "nodemon --exec ts-node src/database/mysql/prisma/index.ts",
    "dev:ddd-clean": "nodemon --exec ts-node src/architecture/code/ddd-clean-architecture/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint ./src/**/.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@bufbuild/buf": "^1.55.1",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/server": "^4.12.2",
    "@bufbuild/protobuf": "^2.6.0",
    "@grpc/grpc-js": "^1.13.4",
    "@prisma/client": "^6.15.0",
    "amqplib": "^0.10.8",
    "cross-fetch": "^4.1.0",
    "express": "^5.1.0",
    "fastify": "^5.4.0",
    "fastify-cli": "^7.4.0",
    "graphql": "^16.11.0",
    "kafkajs": "^2.2.4",
    "mongodb": "^6.19.0",
    "mysql2": "^3.14.3",
    "redis": "^5.8.1",
    "ts-proto": "^2.7.5",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^4.0.5"
  }
}
